<div class = "row">
    <div class = "col-md-6 col-md-offset-3"
      <%= form_for @portfolio, url: user_portfolio_path do |f| %>
        <div class = "form-group">
          <%= f.label :first_name %>
          <%= f.text_field :first_name, class: 'form-control' %>
        </div>
        <div class = "form-group">
          <%= f.label :last_name %>
          <%= f.text_field :last_name, class: 'form-control' %>
        </div>
      <% end %>
    </div>
</div>
